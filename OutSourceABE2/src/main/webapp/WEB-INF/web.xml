<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>datareg</servlet-name>
        <servlet-class>datareg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>endreg</servlet-name>
        <servlet-class>endreg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Url</servlet-name>
        <servlet-class>Url</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>integrity</servlet-name>
        <servlet-class>integrity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendfile</servlet-name>
        <servlet-class>sendfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewintegrity</servlet-name>
        <servlet-class>viewintegrity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Url1</servlet-name>
        <servlet-class>Url1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>datareg</servlet-name>
        <url-pattern>/datareg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>endreg</servlet-name>
        <url-pattern>/endreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Url</servlet-name>
        <url-pattern>/Url</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>integrity</servlet-name>
        <url-pattern>/integrity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendfile</servlet-name>
        <url-pattern>/sendfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewintegrity</servlet-name>
        <url-pattern>/viewintegrity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Url1</servlet-name>
        <url-pattern>/Url1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
